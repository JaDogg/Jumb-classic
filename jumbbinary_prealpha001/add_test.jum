;Compile as console

DEFNUM A1 A2 A3 A4

TITLE("SIMPLE PLUS")

LETINT A1 500
LETINT A2 1000
LETINT A3 A1 + 50 + A2
LETINT A4 A3

ECHO("A1 => ")
ECHOINT(A1)
ECHONEWLINE

ECHO("A2 => ")
ECHOINT(A2)
ECHONEWLINE

ECHO("A3 = A1 + 50 + A2 => ")
ECHOINT(A3)
ECHONEWLINE

ECHO("A4 = A3 => ")
ECHOINT(A4)
ECHONEWLINE

ECHO("A4 = A4 + 50 => ")
LETINT A4 A4 + 50
ECHOINT(A4)
ECHONEWLINE


ECHO("DO YOU LIKE PLUS +++")

PAUSE 
